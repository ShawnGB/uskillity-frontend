{
    "presets": ["react-app"],
    "plugins": [
        ["module-resolver", {
            "root": ["."],
            "cwd": "src",
            "alias": {
                "app:routes": "./routes",
                "app:components": "./components",
                "app:utils": "./utils",
                "app:service": "./service",
                "app:containers": "./containers",
                "app:store": "./store",
            },
            "extensions": [".jsx"]
        }]
    ],
}
