{
    "presets": ["react-app"],
    "plugins": [
        ["module-resolver", {
            "root": ["."],
            "cwd": "src",
            "alias": {
                "app:routes": "./routes",
                "app:components": "./components",
                "app:utils": "./utils",
                "app:containers": "./containers",
                "app:store": "./store",
            },
            "extensions": [".jsx"]
        }],
        ],
    "env": {
      "production": {
          "plugins": ["transform-remove-console"],
      }
    }
}
